{
  "name": "lego-expo",
  "description": "Play with Lego brix anywhere using Expo",
  "main": "node_modules/expo/AppEntry.js",
  "author": "Evan Bacon",
  "license": "MIT",
  "homepage": "https://github.com/evanbacon/Lego-Expo#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evanbacon/Lego-Expo.git"
  },
  "bugs": {
    "url": "https://github.com/evanbacon/Lego-Expo/issues"
  },
  "sideEffects": [
    "screens/HomeScreen.js"
  ],
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo build:web",
    "eject": "expo eject",
    "test": "jest --watchAll",
    "update-webpack-config": "cp -r ../../expo-cli/packages/webpack-config/ node_modules/\\@expo/webpack-config/",
    "deploy": "netlify deploy --dir web-build",
    "postinstall": "node ./ad.js"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/samples": "~3.0.0",
    "@expo/vector-icons": "^10.0.1",
    "expo": "^33.0.0",
    "expo-asset": "^5.0.0",
    "expo-constants": "^5.0.0",
    "expo-font": "^5.0.0",
    "expo-gl": "^6.0.0",
    "expo-three": "^4.0.6",
    "expo-web-browser": "^5.0.0",
    "react": "16.8.3",
    "react-dom": "^16.8.6",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-web": "^0.11.4",
    "react-native-web-hooks": "^1.0.2",
    "react-navigation": "^3.11.0",
    "three": "^0.125.0"
  },
  "devDependencies": {
    "@expo/webpack-config": "^0.7.4",
    "babel-preset-expo": "^6.0.0",
    "chalk": "^2.4.2",
    "jest-expo": "^33.0.0"
  },
  "private": true
}
